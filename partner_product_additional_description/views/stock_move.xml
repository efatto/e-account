<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="stock_move_tree_additional_description" model="ir.ui.view">
            <field name="name">stock.move.tree.additional.description</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']"
                       position="after">
                    <field name="partner_product_additional_description_id"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
